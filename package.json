{
  "name": "habitrack",
  "version": "0.1.0",
  "author": "Dominik Hryshaiev <domhryshaiev@gmail.com>",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/domhhv/habitrack.git"
  },
  "description": "A simple calendar tracker for your habits",
  "homepage": "https://www.habitrack.io",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "zustand",
    "eslint",
    "prettier",
    "react-aria",
    "heroui",
    "tailwind",
    "supabase"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn typecheck && yarn eslint:check && yarn prettier:check",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview --open",
    "typecheck": "tsc --noEmit",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "lint:sql": "node scripts/sqlfluff.js lint",
    "fix:sql": "node scripts/sqlfluff.js fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn test --coverage",
    "test:no-cache": "yarn test --no-cache",
    "supabase": "node scripts/supabase-cli.js",
    "db:reset": "yarn supabase db reset",
    "db:start": "yarn supabase start -x vector",
    "db:stop": "yarn supabase stop",
    "db:status": "yarn supabase status",
    "db:restart": "yarn db:stop && yarn db:start",
    "db:diff": "yarn supabase db diff",
    "db:diff:storage": "yarn supabase db diff --schema storage",
    "db:migration:up": "yarn supabase migration up",
    "db:migration:new": "yarn supabase migration new",
    "db:gen-types": "yarn supabase gen types --lang=typescript --schema public --schema auth --schema storage --local > supabase/database.types.ts; yarn eslint:fix supabase/database.types.ts"
  },
  "dependencies": {
    "@heroui/react": "^2.8.5",
    "@internationalized/date": "^3.10.0",
    "@phosphor-icons/react": "^2.1.10",
    "@supabase/supabase-js": "^2.87.1",
    "@vercel/analytics": "^1.6.1",
    "browser-image-compression": "^2.0.2",
    "camelcase-keys": "^10.0.1",
    "decamelize-keys": "^2.0.1",
    "framer-motion": "^12.23.26",
    "immer": "^11.0.1",
    "lodash.capitalize": "^4.2.1",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "pluralize": "^8.0.0",
    "react": "^19.2.3",
    "react-aria": "^3.44.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.3",
    "react-router": "^7.10.1",
    "react-stately": "^3.42.0",
    "type-fest": "^5.3.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@relative-ci/agent": "^4.3.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/lodash.capitalize": "^4.2.9",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.keyby": "^4.6.9",
    "@types/node": "^22.18.3",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-switch-case": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rollup-plugin-visualizer": "^6.0.3",
    "rollup-plugin-webpack-stats": "^2.1.4",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^22.15.0"
  },
  "packageManager": "yarn@4.9.4"
}
