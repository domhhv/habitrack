{
  "name": "habitrack",
  "version": "0.1.0",
  "author": "Dominik Hryshaiev <domhryshaiev@gmail.com>",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/domhhv/habitrack.git"
  },
  "description": "A simple calendar tracker for your habits",
  "homepage": "https://www.habitrack.io",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "zustand",
    "eslint",
    "prettier",
    "react-aria",
    "heroui",
    "tailwind",
    "date-fns",
    "supabase",
    "jest"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn typecheck && yarn eslint:check && yarn prettier:check",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview --open",
    "typecheck": "tsc --noEmit",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn test --coverage",
    "test:no-cache": "yarn test --no-cache",
    "db:reset": "supabase db reset",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:restart": "yarn db:stop && yarn db:start",
    "db:diff": "supabase db diff",
    "db:migration:up": "supabase migration up",
    "db:migration:new": "supabase migration new",
    "db:gen-types": "supabase gen types --lang=typescript --schema public --schema auth --schema storage --local > supabase/database.types.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "typecheck",
      "eslint",
      "prettier --check"
    ]
  },
  "dependencies": {
    "@heroui/react": "^2.7.4",
    "@internationalized/date": "^3.7.0",
    "@phosphor-icons/react": "^2.1.7",
    "@supabase/supabase-js": "^2.49.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.7",
    "react": "^19.0.0",
    "react-aria": "^3.37.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0",
    "react-stately": "^3.35.0",
    "type-fest": "^4.35.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^18.11.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.0.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup-plugin-visualizer": "^5.14.0",
    "supabase": "^2.15.8",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": "^18.20.7",
    "yarn": "^4.6.0"
  },
  "packageManager": "yarn@4.7.0"
}
