{
  "name": "habitrack",
  "version": "0.1.0",
  "author": "Dominik Hryshaiev <domhryshaiev@gmail.com>",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/domhhv/habitrack.git"
  },
  "description": "A simple calendar tracker for your habits",
  "homepage": "https://www.habitrack.io",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "zustand",
    "eslint",
    "prettier",
    "react-aria",
    "heroui",
    "tailwind",
    "date-fns",
    "supabase",
    "jest"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "yarn typecheck && yarn eslint:check && yarn prettier:check",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview --open",
    "typecheck": "tsc --noEmit",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "lint:sql": "node scripts/sqlfluff.js lint",
    "fix:sql": "node scripts/sqlfluff.js fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "yarn test --coverage",
    "test:no-cache": "yarn test --no-cache",
    "supabase": "node scripts/supabase-cli.js",
    "db:reset": "yarn supabase db reset",
    "db:start": "yarn supabase start -x vector",
    "db:stop": "yarn supabase stop",
    "db:status": "yarn supabase status",
    "db:restart": "yarn db:stop && yarn db:start",
    "db:diff": "yarn supabase db diff",
    "db:diff:storage": "yarn supabase db diff --schema storage",
    "db:migration:up": "yarn supabase migration up",
    "db:migration:new": "yarn supabase migration new",
    "db:gen-types": "yarn supabase gen types --lang=typescript --schema public --schema auth --schema storage --local > supabase/database.types.ts; yarn eslint:fix supabase/database.types.ts"
  },
  "dependencies": {
    "@heroui/react": "^2.7.8",
    "@internationalized/date": "^3.8.0",
    "@phosphor-icons/react": "^2.1.7",
    "@supabase/supabase-js": "^2.49.4",
    "@vercel/analytics": "^1.5.0",
    "browser-image-compression": "^2.0.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.9.7",
    "immer": "^10.1.1",
    "pluralize": "^8.0.0",
    "react": "^19.1.0",
    "react-aria": "^3.39.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "react-stately": "^3.37.0",
    "type-fest": "^4.37.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@relative-ci/agent": "^4.3.1",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.12",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "3.1.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-switch-case": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup-plugin-visualizer": "^5.14.0",
    "rollup-plugin-webpack-stats": "^2.0.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": "^22.15.0"
  },
  "packageManager": "yarn@4.9.1"
}
